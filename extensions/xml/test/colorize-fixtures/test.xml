        <hr:born>1952-03-03</hr:born>
        <qualification>bossy, crabby and selfish</qualification>
   </character>
    <VisualState.Setters>
        <Setter Target="inputPanel.Orientation" Value="Vertical"/>
        <Setter Target="inputButton.Margin" Value="0,4,0,0"/>
    </VisualState.Setters>
</project>
