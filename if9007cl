+     !window.Symbol || !Array.prototype.includes) {
+   // load polyfills dynamically
+-  import('../mastodon/polyfills').then(main);
++  import('../mastodon/polyfills').then(main).catch(e => {
++    console.error(e) // eslint-disable-line no-console
++  });
+ } else {
+   main();
+ }
